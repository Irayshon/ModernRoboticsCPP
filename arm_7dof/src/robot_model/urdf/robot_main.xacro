<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="robot_inertia.xacro" />


  <link name="base_link">

    <!-- Visual -->
    <visual>
      <origin xyz="0 0 0.5" rpy="0 0 0"/>
      <material name="black" />
      <geometry>
        <cylinder radius="3" length="1"/>
      </geometry>
    </visual>

    <!-- Collision -->
    <collision>
      <origin xyz="0 0 0.5" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="3" length="1"/>
      </geometry>
    </collision>

    <xacro:cylinder_inertia m="100.0" r="3" h="1"/>

  </link>


  <link name="joint1_link">

    <!-- Visual -->
    <visual>
      <origin xyz="0 0 2" rpy="0 0 0"/>
      <material name="grey" />
      <geometry>
        <box size="1 1 4" />
      </geometry>
       
    </visual>

    <!-- Collision -->
    <collision>
      <origin xyz="0 0 2" rpy="0 0 0"/>
      <geometry>
        <box size="1 1 4" />
      </geometry>
    </collision>

    <xacro:box_inertia m="2.0" x="1" y="1" z="4"/>

  </link>

   <joint name="joint1" type="revolute">
        <parent link="base_link" />
        <child link="joint1_link" />
        <origin xyz="0 0 1" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit effort="1000.0" velocity="1.0" lower="-3.14" upper="3.14" />
    </joint>



    <link name="joint2_link">

    <!-- Visual -->
    <visual>
      <origin xyz="1 0 0" rpy="0 1.57 0"/>
      <material name="black" />
      <geometry>
        <box size="1 1 2" />
      </geometry>
    </visual>

    <!-- Collision -->
