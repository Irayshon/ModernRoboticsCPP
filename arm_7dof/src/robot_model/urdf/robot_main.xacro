      <origin xyz="1 0 0" rpy="0 1.57 0"/>
      <geometry>
        <box size="1 1 2" />
      </geometry>
    </collision>

    <xacro:box_inertia m="2.0" x="1" y="1" z="2"/>

  </link>

   <joint name="joint2" type="fixed">
        <parent link="joint1_link" />
        <child link="joint2_link" />
        <origin xyz="0 0 4" rpy="0 0 0" />
  </joint>



